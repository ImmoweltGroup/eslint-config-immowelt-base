{
  "name": "@immowelt/eslint-config-immowelt-base",
  "version": "0.0.0",
  "description": "ESLint shareable base config",
  "main": "index.js",
  "repository": "https://github.com/ImmoweltGroup/eslint-config-immowelt-base.git",
  "author": "Kevin Ruff <kevin.ruff@immonet.de>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@immowelt/browserslist-config": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0"
  },
  "devDependencies": {
    "semantic-release": "^15.9.17"
  },
  "scripts": {
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        },
        {
          "type": "PATCH",
          "release": "patch"
        }
      ]
    }
  },
  "keywords": [
    "eslint",
    "config",
    "immowelt",
    "base"
  ],
  "browserslist": [
    "extends @immowelt/browserslist-config"
  ],
  "engines": {
    "node": ">=12.22.6",
    "yarn": ">=1.6.0"
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  }
}
